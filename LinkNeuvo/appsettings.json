{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "CrawlerOptions": {
    "BaseUrl": "https://example.com",
    "CsvOutputPath": "output.csv",
    "FlushCsvAtLineCount": 10,
    "MaxTasks": 4,
    "TimeoutSeconds": 30,
    "FetchExternally": false,
    "CrawlExtensions": [
      ".htm",
      ".html"
    ],
    "CrawlNoExtension": true
  }
}